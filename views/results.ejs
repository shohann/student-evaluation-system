<%- include('partials/header'); -%>


<div class="current-page" style="border-bottom: 1px solid white; border-top: 1px solid white; padding-top: 10px;">
	<div>
		<h1>Results</h1>
		<br>
	</div>
</div>
<br>



<br>
<br>

<table class="container">
	<thead>
		<tr>
			<th><h1>Name</h1></th>
			<th><h1>Category</h1></th>
			<th><h1>Total Marks</h1></th>
			<th><h1>Obtained Marks</h1></th>
			<th><h1>Details</h1></th>
		</tr>
	</thead>

	<tbody>
		<% if (results.length > 0) { %>
			<% for(let i = 0; i < results.length; i++ ){ %>
				<tr>
					<td><%= results[i].quiz.name %></td>
					<td><%= results[i].quiz.category_name %> TEST</td>
					<td><%= results[i].quiz.full_marks %></td>
					<td><%= results[i].marks %></td>
					<td><a style="border-bottom: 1px solid white;" href="/api/responses/<%= results[i].quiz.id %>">View</a></td>
				</tr>
			<% } %>
		<% } else { %>
				<!-- <p>No results</p> -->
		<% } %>
	</tbody>
</table>

