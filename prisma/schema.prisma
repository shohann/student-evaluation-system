generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Quiz {
  id Int @id @default(autoincrement())
}

model Question {
  id Int @id @default(autoincrement())
}

model Option {
  id Int @id @default(autoincrement())
}

model Response {
  id Int @id @default(autoincrement())
}

model Result {
  id Int @id @default(autoincrement())
}
